{
    "manifest_version": 3,
    "name": "BookMark Extension",
    "version": "1.0",
    "description": "Build a extension that helps to manage your webpages and notes",
    "action":{
        "default_popup": "popup.html"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background":{
        "service_worker": "background.js"
    },
    "permissions": ["storage"]
}