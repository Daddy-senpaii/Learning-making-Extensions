{
    "manifest_version": 3,
    "name": "Tabs Grouper",
    "description": "Used to Groups the tabs",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": ["storage", "tabs", "tabGroups"]
}