{
    "manifest_version" : 3,
    "name": "toggler",
    "description": "Its just toggle between light mode and dark mode",
    "version": "1.0",
    "action":{
        "default_popup": "pop.html"
    },
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background":{
        "service_worker": "background.js"
    },
    "permissions": ["storage"]
}